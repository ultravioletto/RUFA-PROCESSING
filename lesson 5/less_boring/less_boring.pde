int yStart = 200;
int h = 20;
float step = 45;//45.4545454545;

void setup() {
  size (500, 500);
}


void draw() {
  background(255);
  line (1*step, yStart, 1*step, yStart+h);
  line (2*step, yStart, 2*step, yStart+h);
  line (3*step, yStart, 3*step, yStart+h);
  line (4*step, yStart, 4*step, yStart+h);
  line (5*step, yStart, 5*step, yStart+h);
  line (6*step, yStart, 6*step, yStart+h);
  line (7*step, yStart, 7*step, yStart+h);
  line (8*step, yStart, 8*step, yStart+h);
  line (9*step, yStart, 9*step, yStart+h);
  line (10*step, yStart, 10*step, yStart+h);
}
